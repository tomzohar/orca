ANTHROPIC_API_KEY=your-api-key

# HTTP Request Logging
# Enable/disable HTTP request/response logging
ENABLE_RESPONSE_LOGGING=true

# Log level for HTTP requests (options: debug, log, verbose)
# - debug: Most verbose, includes full metadata as JSON
# - log: Standard logging (recommended)
# - verbose: Between debug and log
RESPONSE_LOG_LEVEL=log

# Comma-separated URL patterns to exclude from logging
# Default excludes health checks and SSE endpoints
RESPONSE_LOG_EXCLUDE_PATHS=/health,/events